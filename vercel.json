{
  "version": 2,
  "name": "blockclaim-app",
  "builds": [
    {
      "src": "server/simple.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/simple.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/simple.js": {
      "maxDuration": 30
    }
  }
}